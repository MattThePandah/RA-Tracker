VITE_APP_NAME=Pannboo

# Point to your local proxy (server/index.js) for IGDB token & images
VITE_IGDB_PROXY_URL=http://localhost:8787
VITE_IGDB_ENABLED=true

# RetroAchievements (optional; can also set in Settings UI)
VITE_RA_ENABLED=true
VITE_RA_USERNAME=
VITE_RA_API_KEY=

# Messenger Panda (optional notifications)
DISCORD_WEBHOOK_URL=
STREAMERBOT_URL=http://localhost:8080
STREAMERBOT_ENABLED=false

# StreamerBot command API (server)
STREAMERBOT_API_KEY=

# Cloudflare Turnstile for public suggestions
VITE_TURNSTILE_SITE_KEY=

# Overlay token (optional if you pass ?token= in overlay URLs)
VITE_OVERLAY_TOKEN=

# Optional: enable newer server-backed UI/features
VITE_V2_ENABLED=false
VITE_ALL_CONSOLES_ENABLED=false
